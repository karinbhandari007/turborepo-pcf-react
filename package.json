{
  "name": "dme",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dme-react:dev": "turbo dev --filter dme-react",
    "dme-pcf:dev": "turbo dev --filter dme-pcf",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.4",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
